// print A-Frame which is length of Bs + 2
// print newline if you reached end of length
// print a 
// for length of b print b
// print a and newline 

#include <stdio.h>
#include <stdlib.h>

int main()
{
	// print A-frame 
	int breite = 6;
	int hoehe = 3;

	for (int i = 0; i < breite + 2; ++i)
	{
		printf("A");
	}

	for (int i = 0; i < hoehe; ++i)
	{
		printf("\n");
		printf("A");

		for (int j = 0; j < breite; ++j)
		{
			printf("B");
		}
		printf("A");
	}

	printf("\n");
	for (int i = 0; i < breite + 2; ++i)
	{
		printf("A");
	}

	return 0;
}